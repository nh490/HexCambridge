{
    "manifest_version": 2,
  
    "name": "Censor offensive words",
    "description": "Click a button to censor the offensive words in each category",
    "version": "1.0",
  
    "icons": {
      "48": "icon.png"
    },
  
    "permissions": [
      "tabs", "<all_urls>"
    ],
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    }
  }
  